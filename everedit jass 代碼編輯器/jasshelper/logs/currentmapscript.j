    call SaveReal(YDHT, GetHandleId(t1), 0xA3098AE2, r1)
    else
    set x2=( x1 + ( r1 * Cos(Deg2Rad(LoadReal(YDHT, GetHandleId(t1), 0x9CD60476))) ) )
    set y2=( y1 + ( r1 * Sin(Deg2Rad(LoadReal(YDHT, GetHandleId(t1), 0x9CD60476))) ) )
    call KillUnit(u2)
    call FlushChildHashtable(YDHT, GetHandleId(t1))
    call DestroyTimer(t1)
    endif
    // ---------------------------------
    call SetUnitX(u1, x2)
    call SetUnitY(u1, y2)
    call SetUnitX(u2, x2)
    call SetUnitY(u2, y2)
    // ---------------------------------
    set t1=null
