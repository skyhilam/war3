function Trig_Item04Actions takes nothing returns nothing
    local group ydl_group
    local unit ydl_unit
    set udg_A_Common_temp1=GetSpellTargetLoc()
    // ---------------------------------
    set ydl_group=CreateGroup()
    call GroupEnumUnitsInRange(ydl_group, GetLocationX(udg_A_Common_temp1), GetLocationY(udg_A_Common_temp1), 440.00, null)
    loop
        set ydl_unit=FirstOfGroup(ydl_group)
        exitwhen ydl_unit == null
        call GroupRemoveUnit(ydl_group, ydl_unit)
        if ( ( IsUnitType(ydl_unit, UNIT_TYPE_STRUCTURE) == false ) and ( IsUnitType(ydl_unit, UNIT_TYPE_MAGIC_IMMUNE) == false ) and ( IsUnitEnemy(ydl_unit, GetOwningPlayer(GetTriggerUnit())) == true ) and ( GetUnitPointValue(ydl_unit) != 1 ) and ( IsUnitAliveBJ(ydl_unit) == true ) ) then
            call UnitDamageTargetBJ(GetTriggerUnit(), ydl_unit, 400.00, ATTACK_TYPE_NORMAL, DAMAGE_TYPE_MAGIC)
        else
        endif
    endloop
    call DestroyGroup(ydl_group)
    // ---------------------------------
    call DestroyEffect(AddSpecialEffectLoc("Objects\\Spawnmodels\\Other\\NeutralBuildingExplosion\\NeutralBuildingExplosion.mdl", udg_A_Common_temp1))
    // ☆排??
    call RemoveLocation(udg_A_Common_temp1)
    set ydl_group=null
    set ydl_unit=null
endfunction
