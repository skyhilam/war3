function Trig_KnockBackFunc011T takes nothing returns nothing
    call SaveBoolean(YDHT, GetHandleId(GetExpiredTimer()), 0x9C0F555E, false)
    call SaveReal(YDHT, GetHandleId(GetExpiredTimer()), 0xA99320FA, ( GetUnitX(LoadUnitHandle(YDHT, GetHandleId(GetExpiredTimer()), 0xC303079D)) + ( LoadReal(YDHT, GetHandleId(GetExpiredTimer()), 0x9CD60476) * Cos(Deg2Rad(LoadReal(YDHT, GetHandleId(GetExpiredTimer()), 0xA1614B4D))) ) ))
    call SaveReal(YDHT, GetHandleId(GetExpiredTimer()), 0xFDF65382, ( GetUnitY(LoadUnitHandle(YDHT, GetHandleId(GetExpiredTimer()), 0xC303079D)) + ( LoadReal(YDHT, GetHandleId(GetExpiredTimer()), 0x9CD60476) * Sin(Deg2Rad(LoadReal(YDHT, GetHandleId(GetExpiredTimer()), 0xA1614B4D))) ) ))
    call SaveReal(YDHT, GetHandleId(GetExpiredTimer()), 0x1FB8CFB5, 0.00)
    call SaveReal(YDHT, GetHandleId(GetExpiredTimer()), 0x073298C5, 0.00)
    if ( ( LoadBoolean(YDHT, GetHandleId(GetExpiredTimer()), 0x96900F29) == true ) ) then
        if ( ( IsTerrainPathable(LoadReal(YDHT, GetHandleId(GetExpiredTimer()), 0xA99320FA), LoadReal(YDHT, GetHandleId(GetExpiredTimer()), 0xFDF65382), PATHING_TYPE_WALKABILITY) == false ) ) then
            call SetUnitX(LoadUnitHandle(YDHT, GetHandleId(GetExpiredTimer()), 0xC303079D), LoadReal(YDHT, GetHandleId(GetExpiredTimer()), 0xA99320FA))
            call SetUnitY(LoadUnitHandle(YDHT, GetHandleId(GetExpiredTimer()), 0xC303079D), LoadReal(YDHT, GetHandleId(GetExpiredTimer()), 0xFDF65382))
        else
            call SaveBoolean(YDHT, GetHandleId(GetExpiredTimer()), 0x9C0F555E, true)
        endif
    else
        call SetUnitPosition(LoadUnitHandle(YDHT, GetHandleId(GetExpiredTimer()), 0xC303079D), LoadReal(YDHT, GetHandleId(GetExpiredTimer()), 0xA99320FA), LoadReal(YDHT, GetHandleId(GetExpiredTimer()), 0xFDF65382))
        call SaveReal(YDHT, GetHandleId(GetExpiredTimer()), 0x1FB8CFB5, ( GetUnitX(LoadUnitHandle(YDHT, GetHandleId(GetExpiredTimer()), 0xC303079D)) - LoadReal(YDHT, GetHandleId(GetExpiredTimer()), 0xA99320FA) ))
        call SaveReal(YDHT, GetHandleId(GetExpiredTimer()), 0x073298C5, ( GetUnitY(LoadUnitHandle(YDHT, GetHandleId(GetExpiredTimer()), 0xC303079D)) - LoadReal(YDHT, GetHandleId(GetExpiredTimer()), 0xFDF65382) ))
        if ( ( SquareRoot(( ( LoadReal(YDHT, GetHandleId(GetExpiredTimer()), 0x1FB8CFB5) * LoadReal(YDHT, GetHandleId(GetExpiredTimer()), 0x1FB8CFB5) ) + ( LoadReal(YDHT, GetHandleId(GetExpiredTimer()), 0x073298C5) * LoadReal(YDHT, GetHandleId(GetExpiredTimer()), 0x073298C5) ) )) > LoadReal(YDHT, GetHandleId(GetExpiredTimer()), 0x9CD60476) ) ) then
            if ( ( LoadBoolean(YDHT, GetHandleId(GetExpiredTimer()), 0xD740D5AE) == true ) ) then
                call SaveReal(YDHT, GetHandleId(GetExpiredTimer()), 0xA1614B4D, Atan2BJ(( GetUnitX(LoadUnitHandle(YDHT, GetHandleId(GetExpiredTimer()), 0xC303079D)) - LoadReal(YDHT, GetHandleId(GetExpiredTimer()), 0xFDF65382) ), ( GetUnitX(LoadUnitHandle(YDHT, GetHandleId(GetExpiredTimer()), 0xC303079D)) - LoadReal(YDHT, GetHandleId(GetExpiredTimer()), 0xA99320FA) )))
            else
                call SaveBoolean(YDHT, GetHandleId(GetExpiredTimer()), 0x9C0F555E, true)
            endif
        else
        endif
    endif
    call SaveReal(YDHT, GetHandleId(GetExpiredTimer()), 0xDD66C616, ( LoadReal(YDHT, GetHandleId(GetExpiredTimer()), 0xDD66C616) + LoadReal(YDHT, GetHandleId(GetExpiredTimer()), 0x25DAB820) ))
    if ( ( ( LoadReal(YDHT, GetHandleId(GetExpiredTimer()), 0xDD66C616) >= LoadReal(YDHT, GetHandleId(GetExpiredTimer()), 0x312C4181) ) or ( LoadBoolean(YDHT, GetHandleId(GetExpiredTimer()), 0x9C0F555E) == true ) ) ) then
