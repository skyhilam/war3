function RoseTime takes nothing returns nothing
local trigger tr
local timer t= GetExpiredTimer()
local integer h2i= GetHandleId(t)
local unit u1=LoadUnitHandle(YDHT, h2i, 0xA1614B4D)
local real v= LoadReal(YDHT, h2i, 0x9C0F555E)
local boolean b= LoadBoolean(YDHT, h2i, 0xFDF65382)
local real x= GetUnitX(u1)
local real y= GetUnitY(u1)
local real x2= LoadReal(YDHT, h2i, 0x073298C5)
local real y2= LoadReal(YDHT, h2i, 0xA99320FA)
local real a= Atan2(y2 - y, x2 - x)
local real x1= x + v * Cos(a)
local real y1= y + v * Sin(a)
local real dt= SquareRoot(( Pow(( x2 - x ), 2) ) + Pow(( y2 - y ), 2))
if ( dt < v ) then
set x1=x + dt * Cos(a)
set y1=y + dt * Sin(a)
    //假如 B ??false
    if ( ( b == true ) or ( IsTerrainPathable(x1, y1, PATHING_TYPE_WALKABILITY) == true ) ) then
	call SetUnitX(u1, x1)
    call SetUnitY(u1, y1)
    else
    endif

set tr=LoadTriggerHandle(YDHT, h2i, 0x96900F29)
call ConditionalTriggerExecute(tr)
call SetUnitTimeScalePercent(u1, 100.00)
call FlushChildHashtable(YDHT, h2i)
call DestroyTimer(t)

else
call SetUnitX(u1, x1)
call SetUnitY(u1, y1)
endif
set t=null
set u1=null
set tr=null
endfunction
function Rosecharge takes unit u,real x,real y,real x2,real y2,real v,real t1,boolean b,trigger tr returns nothing
local timer t= CreateTimer()
local integer h2i= GetHandleId(t)
    call SaveUnitHandle(YDHT, h2i, 0xA1614B4D, u)
    //是否要z??"可通行"
    call SaveBoolean(YDHT, h2i, 0xFDF65382, b)
    call SaveTriggerHandle(YDHT, h2i, 0x96900F29, tr)
    call SaveReal(YDHT, h2i, 0x9C0F555E, v)
    call SaveReal(YDHT, h2i, 0x073298C5, x2)
    call SaveReal(YDHT, h2i, 0xA99320FA, y2)
    call TimerStart(t, t1, true, function RoseTime)
set t=null
endfunction
