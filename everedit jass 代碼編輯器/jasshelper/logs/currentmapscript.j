function Trig_Kick_sys_AActions takes nothing returns nothing
    local integer ydl_localvar_step= LoadInteger(YDHT, GetHandleId(GetTriggeringTrigger()), 0xCFDE6C76)
    set ydl_localvar_step=ydl_localvar_step + 3
    call SaveInteger(YDHT, GetHandleId(GetTriggeringTrigger()), 0xCFDE6C76, ydl_localvar_step)
    call SaveInteger(YDHT, GetHandleId(GetTriggeringTrigger()), 0xECE825E7, ydl_localvar_step)
    call DisableTrigger(GetTriggeringTrigger())
    set udg_KickVotesYes=0
    set udg_Kick_time=S2I(SubStringBJ(GetEventPlayerChatString(), 7, 8))
    call DisplayTimedTextToForce(GetPlayersAll(), 5.00, ( ( udg_S03_color_of_players[GetConvertedPlayerId(GetTriggerPlayer())] + GetPlayerName(GetTriggerPlayer()) ) + ( ( "|rl起了" + udg_S03_color_of_players[udg_Kick_time] ) + ( GetPlayerName(ConvertedPlayer(udg_Kick_time)) + "|r的踢除投票?? ) ) ))
    call DisplayTimedTextToForce(GetPlayersAll(), 5.00, "TRIGSTR_12412")
    set udg_KickTime=0
    set bj_forLoopAIndex=1
    set bj_forLoopAIndexEnd=10
    loop
        exitwhen bj_forLoopAIndex > bj_forLoopAIndexEnd
        if ( ( GetPlayerController(ConvertedPlayer(GetForLoopIndexA())) == MAP_CONTROL_USER ) and ( GetPlayerSlotState(GetOwningPlayer(udg_S16_x[GetForLoopIndexA()])) == PLAYER_SLOT_STATE_PLAYING ) ) then
            set udg_KickTime=( udg_KickTime + 1 )
        else
        endif
        set bj_forLoopAIndex=bj_forLoopAIndex + 1
    endloop
    if ( ( udg_KickTime < 6 ) ) then
        call QuestMessageBJ(GetPlayersAll(), bj_QUESTMESSAGE_WARNING, "TRIGSTR_2019")
        call DisableTrigger(GetTriggeringTrigger())
        call FlushChildHashtable(YDHT, GetHandleId(GetTriggeringTrigger()) * ydl_localvar_step)
        return
    else
    endif
    call EnableTrigger(gg_trg_Kick_sys_B)
    call YDWEPolledWaitNull(45.00)
    call SaveInteger(YDHT, GetHandleId(GetTriggeringTrigger()), 0xECE825E7, ydl_localvar_step)
    if ( ( udg_KickVotesYes >= ( udg_KickTime - 4 ) ) ) then
        call CustomDefeatBJ(ConvertedPlayer(udg_Kick_time), "TRIGSTR_9249")
        // ---------------------------------
        // 因惩嬲叩年列在用到太多??所以先用捍嬲荡嫫
        set udg_A_temp_Number[0]=udg_Kick_time
        // ??0Υ娉员量(之後多重面板也用到@??
        call QuestMessageBJ(GetPlayersAll(), bj_QUESTMESSAGE_WARNING, ( udg_S03_color_of_players[udg_Kick_time] + ( GetPlayerName(ConvertedPlayer(udg_Kick_time)) + ( "" + ( "|R已被踢除[??!" ) ) ) ))
        // ---------------------------------
        call SaveInteger(YDHT, GetHandleId(GetTriggeringTrigger()) * ydl_localvar_step, 0xA1614B4D, 0)
        // ---------------------------------
        set bj_forLoopAIndex=1
        set bj_forLoopAIndexEnd=10
        loop
            exitwhen bj_forLoopAIndex > bj_forLoopAIndexEnd
            if ( ( GetPlayerController(ConvertedPlayer(GetForLoopIndexA())) == MAP_CONTROL_USER ) and ( GetPlayerSlotState(GetOwningPlayer(udg_S16_x[GetForLoopIndexA()])) == PLAYER_SLOT_STATE_PLAYING ) and ( IsUnitAlly(udg_S16_x[GetForLoopIndexA()], ConvertedPlayer(udg_Kick_time)) == true ) ) then
                call SaveInteger(YDHT, GetHandleId(GetTriggeringTrigger()) * ydl_localvar_step, 0xA1614B4D, ( LoadInteger(YDHT, GetHandleId(GetTriggeringTrigger()) * ydl_localvar_step, 0xA1614B4D) + 1 ))
            else
            endif
            set bj_forLoopAIndex=bj_forLoopAIndex + 1
        endloop
        // ---------------------------------
        set bj_forLoopAIndex=1
        set bj_forLoopAIndexEnd=10
        loop
            exitwhen bj_forLoopAIndex > bj_forLoopAIndexEnd
            if ( ( GetPlayerSlotState(GetOwningPlayer(udg_S16_x[GetForLoopIndexA()])) == PLAYER_SLOT_STATE_PLAYING ) and ( IsUnitAlly(udg_S16_x[GetForLoopIndexA()], ConvertedPlayer(udg_Kick_time)) == true ) ) then
                call AdjustPlayerStateBJ(( GetPlayerState(ConvertedPlayer(udg_Kick_time), PLAYER_STATE_RESOURCE_LUMBER) / LoadInteger(YDHT, GetHandleId(GetTriggeringTrigger()) * ydl_localvar_step, 0xA1614B4D) ), GetOwningPlayer(udg_S16_x[GetForLoopIndexA()]), PLAYER_STATE_RESOURCE_LUMBER)
                call AdjustPlayerStateBJ(( GetPlayerState(ConvertedPlayer(udg_Kick_time), PLAYER_STATE_RESOURCE_GOLD) / LoadInteger(YDHT, GetHandleId(GetTriggeringTrigger()) * ydl_localvar_step, 0xA1614B4D) ), GetOwningPlayer(udg_S16_x[GetForLoopIndexA()]), PLAYER_STATE_RESOURCE_GOLD)
            else
            endif
            set bj_forLoopAIndex=bj_forLoopAIndex + 1
        endloop
        // ---------------------------------
        call SetPlayerStateBJ(ConvertedPlayer(udg_Kick_time), PLAYER_STATE_RESOURCE_GOLD, 0)
        call SetPlayerStateBJ(ConvertedPlayer(udg_Kick_time), PLAYER_STATE_RESOURCE_LUMBER, 0)
        // ---------------------------------
        // ---------------------------------
        // ---------------------------------
        // x_[
        set bj_forLoopAIndex=1
        set bj_forLoopAIndexEnd=10
        loop
            exitwhen bj_forLoopAIndex > bj_forLoopAIndexEnd
            if ( ( GetPlayerSlotState(GetOwningPlayer(udg_S16_x[GetForLoopIndexA()])) == PLAYER_SLOT_STATE_LEFT ) ) then
                set bj_forLoopBIndex=0
                set bj_forLoopBIndexEnd=10
                loop
                    exitwhen bj_forLoopBIndex > bj_forLoopBIndexEnd
                    call MultiboardSetItemValueColor(MultiboardGetItem(udg_DMB, GetForLoopIndexA(), GetForLoopIndexB()), 55, 55, 55, 255)
                    set bj_forLoopBIndex=bj_forLoopBIndex + 1
                endloop
            else
            endif
            set bj_forLoopAIndex=bj_forLoopAIndex + 1
        endloop
        // ---------------------------------
        set bj_forLoopAIndex=1
        set bj_forLoopAIndexEnd=5
        loop
            exitwhen bj_forLoopAIndex > bj_forLoopAIndexEnd
            if ( ( GetPlayerSlotState(GetOwningPlayer(udg_S16_x[GetForLoopIndexA()])) == PLAYER_SLOT_STATE_LEFT ) ) then
                set bj_forLoopBIndex=1
                set bj_forLoopBIndexEnd=5
                loop
                    exitwhen bj_forLoopBIndex > bj_forLoopBIndexEnd
                    call SetPlayerAllianceStateBJ(GetOwningPlayer(udg_S16_x[GetForLoopIndexA()]), GetOwningPlayer(udg_S16_x[GetForLoopIndexB()]), bj_ALLIANCE_ALLIED_UNITS)
                    call KillUnit(udg_S16_x[GetForLoopIndexA()])
                    set bj_forLoopBIndex=bj_forLoopBIndex + 1
                endloop
            else
            endif
            set bj_forLoopAIndex=bj_forLoopAIndex + 1
        endloop
        set bj_forLoopAIndex=6
        set bj_forLoopAIndexEnd=10
        loop
            exitwhen bj_forLoopAIndex > bj_forLoopAIndexEnd
            if ( ( GetPlayerSlotState(GetOwningPlayer(udg_S16_x[GetForLoopIndexA()])) == PLAYER_SLOT_STATE_LEFT ) ) then
                set bj_forLoopBIndex=6
                set bj_forLoopBIndexEnd=10
                loop
                    exitwhen bj_forLoopBIndex > bj_forLoopBIndexEnd
                    call SetPlayerAllianceStateBJ(GetOwningPlayer(udg_S16_x[GetForLoopIndexA()]), GetOwningPlayer(udg_S16_x[GetForLoopIndexB()]), bj_ALLIANCE_ALLIED_UNITS)
                    call KillUnit(udg_S16_x[GetForLoopIndexA()])
                    set bj_forLoopBIndex=bj_forLoopBIndex + 1
                endloop
            else
            endif
            set bj_forLoopAIndex=bj_forLoopAIndex + 1
        endloop
    else
        call DisplayTimedTextToForce(GetPlayersAll(), 5.00, "TRIGSTR_12421")
    endif
    call DisableTrigger(gg_trg_Kick_sys_B)
    set bj_forLoopAIndex=1
    set bj_forLoopAIndexEnd=10
    loop
        exitwhen bj_forLoopAIndex > bj_forLoopAIndexEnd
        set udg_KickVotesYes_ture[GetForLoopIndexA()]=false
        set bj_forLoopAIndex=bj_forLoopAIndex + 1
    endloop
    call YDWEPolledWaitNull(120.00)
    call SaveInteger(YDHT, GetHandleId(GetTriggeringTrigger()), 0xECE825E7, ydl_localvar_step)
    call EnableTrigger(GetTriggeringTrigger())
    call DisplayTimedTextToForce(GetPlayersAll(), 5.00, "TRIGSTR_12422")
    call FlushChildHashtable(YDHT, GetHandleId(GetTriggeringTrigger()) * ydl_localvar_step)
endfunction
