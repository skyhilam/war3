//===========================================================================
function Trig_Player_VIP_CheckActions takes nothing returns nothing
    local integer ydl_localvar_step= LoadInteger(YDHT, GetHandleId(GetTriggeringTrigger()), 0xCFDE6C76)
    set ydl_localvar_step=ydl_localvar_step + 3
    call SaveInteger(YDHT, GetHandleId(GetTriggeringTrigger()), 0xCFDE6C76, ydl_localvar_step)
    call SaveInteger(YDHT, GetHandleId(GetTriggeringTrigger()), 0xECE825E7, ydl_localvar_step)
    call SaveInteger(YDHT, GetHandleId(GetTriggeringTrigger()) * ydl_localvar_step, 0xA1614B4D, 0)
    set bj_forLoopAIndex=1
    set bj_forLoopAIndexEnd=10
    loop
        exitwhen bj_forLoopAIndex > bj_forLoopAIndexEnd
        set bj_forLoopBIndex=1
        set bj_forLoopBIndexEnd=20
        loop
            exitwhen bj_forLoopBIndex > bj_forLoopBIndexEnd
            if ( ( GetPlayerName(ConvertedPlayer(GetForLoopIndexA())) == udg_N01_name[GetForLoopIndexB()] ) ) then
                call SaveInteger(YDHT, GetHandleId(GetTriggeringTrigger()) * ydl_localvar_step, 0xA1614B4D, ( LoadInteger(YDHT, GetHandleId(GetTriggeringTrigger()) * ydl_localvar_step, 0xA1614B4D) + 1 ))
                call SetPlayerName(ConvertedPlayer(GetForLoopIndexA()), udg_N01_name2[GetForLoopIndexB()])
                set udg_N01_name3[GetForLoopIndexA()]=udg_N01_name2[GetForLoopIndexB()]
                call MultiboardSetItemValue(MultiboardGetItem(udg_DMB, GetForLoopIndexA(), 0), GetPlayerName(GetOwningPlayer(udg_S16_x[GetForLoopIndexA()])))
            else
            endif
            set bj_forLoopBIndex=bj_forLoopBIndex + 1
        endloop
        set bj_forLoopAIndex=bj_forLoopAIndex + 1
    endloop
    if ( ( LoadInteger(YDHT, GetHandleId(GetTriggeringTrigger()) * ydl_localvar_step, 0xA1614B4D) > 0 ) ) then
        call DisplayTimedTextToForce(GetPlayersAll(), 10.00, "TRIGSTR_6820")
    else
    endif
    call DisableTrigger(GetTriggeringTrigger())
    call FlushChildHashtable(YDHT, GetHandleId(GetTriggeringTrigger()) * ydl_localvar_step)
endfunction
