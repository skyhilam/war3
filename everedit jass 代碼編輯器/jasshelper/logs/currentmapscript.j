    call SaveInteger(YDHT, GetHandleId(udg_C01T_T[i]), 0xA3098AE2, ( 8 + 6 * GetUnitAbilityLevel(GetTriggerUnit(), 'A05T') ))
    call TimerStart(udg_C01T_T[i], 0.05, true, function Trig_C01T1Func005T)
