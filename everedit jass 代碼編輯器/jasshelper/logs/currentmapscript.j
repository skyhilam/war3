    call SaveTimerHandle(YDHT, StringHash(("B" )), StringHash(( I2S(LoadInteger(YDHT, GetHandleId(GetTriggeringTrigger()) * ydl_localvar_step, 0xA1614B4D)) )), ( LoadTimerHandle(YDHT, GetHandleId(GetTriggeringTrigger()) * ydl_localvar_step, 0x312C4181))) // INLINED!!
    set ydl_timer=LoadTimerHandle(YDHT, GetHandleId(GetTriggeringTrigger()) * ydl_localvar_step, 0x312C4181)
