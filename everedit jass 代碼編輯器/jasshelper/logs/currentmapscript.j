    call ClearSelectionForPlayer(LoadPlayerHandle(YDHT, GetHandleId(GetTriggeringTrigger()) * ydl_localvar_step, 0x9CD60476))
    call SelectUnitAddForPlayer(udg_A_common_HERO, LoadPlayerHandle(YDHT, GetHandleId(GetTriggeringTrigger()) * ydl_localvar_step, 0x9CD60476))
    // oR??
    call DisableTrigger(gg_trg_its_not_yours)
    call UnitAddItemToSlotById(udg_A_common_HERO, 'fgrg', 0)
