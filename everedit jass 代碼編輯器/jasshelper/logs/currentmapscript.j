    call SaveInteger(YDHT, GetHandleId(GetTriggeringTrigger()) * ydl_localvar_step, 0xA3098AE2, GetConvertedPlayerId(GetOwningPlayer(GetBuyingUnit())))
    call SavePlayerHandle(YDHT, GetHandleId(GetTriggeringTrigger()) * ydl_localvar_step, 0x9CD60476, GetOwningPlayer(GetBuyingUnit()))
    set udg_S01_hero_of_player[LoadInteger(YDHT, GetHandleId(GetTriggeringTrigger()) * ydl_localvar_step, 0xA3098AE2)]=GetSoldUnit()
    set udg_A_common_HERO=GetSoldUnit()
    // ----------------------------------------------------
    if ( ( LoadInteger(YDHT, GetHandleId(GetTriggeringTrigger()) * ydl_localvar_step, 0xA3098AE2) <= 5 ) ) then
        set udg_A_Common_temp1=GetRectCenter(gg_rct_APIG)
        call SetUnitPositionLoc(udg_A_common_HERO, udg_A_Common_temp1)
        call PanCameraToTimedLocForPlayer(LoadPlayerHandle(YDHT, GetHandleId(GetTriggeringTrigger()) * ydl_localvar_step, 0x9CD60476), udg_A_Common_temp1, 0.10)
    else
        set udg_A_Common_temp1=GetRectCenter(gg_rct_BPIG)
        call SetUnitPositionLoc(udg_A_common_HERO, udg_A_Common_temp1)
        call PanCameraToTimedLocForPlayer(LoadPlayerHandle(YDHT, GetHandleId(GetTriggeringTrigger()) * ydl_localvar_step, 0x9CD60476), udg_A_Common_temp1, 0.10)
    endif
    // ----------------------------------------------------
    // 其他玩家看不到已被x走的英雄
    call ForForce(GetPlayersAll(), function Trig_Select_HeroFunc011A)
    // 改分板的英雄大^以及P]其他英雄ν婕业目蛇x??
    set bj_forLoopAIndex=0
    set bj_forLoopAIndexEnd=95
    loop
        exitwhen bj_forLoopAIndex > bj_forLoopAIndexEnd
        if ( ( udg_S01_hero_list[GetForLoopIndexA()] == GetUnitTypeId(udg_A_common_HERO) ) ) then
