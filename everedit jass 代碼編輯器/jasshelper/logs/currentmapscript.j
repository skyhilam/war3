            call SetSoundVolume(GetLastPlayedSound(), 127)
            // ☆增加X
            call AdjustPlayerStateBJ(200, GetOwningPlayer(GetKillingUnitBJ()), PLAYER_STATE_RESOURCE_GOLD)
        else
