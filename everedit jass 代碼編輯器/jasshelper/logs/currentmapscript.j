            // ☆文??
            call DisplayTextToForce(GetPlayersAll(), ( "|cffff0000" + ( GetHeroProperName(udg_S01_hero_of_player[LoadInteger(YDHT, GetHandleId(GetTriggeringTrigger()) * ydl_localvar_step, 0xA1614B4D)]) + "|r拿下了高超的三B! 使耻L?? 他@得~外|cffff9900200|r的p??" ) ))
            // -----------------
