    call EnableTrigger(gg_trg_its_not_yours)
    // 出生特效
    call AddSpecialEffectTargetUnitBJ("origin", udg_A_common_HERO, "Abilities\\Spells\\Other\\Awaken\\Awaken.mdl")
    call DestroyEffect(GetLastCreatedEffectBJ())
    // 排泄
    call RemoveLocation(udg_A_Common_temp1)
    // 英雄D
    call YDWEMultiboardSetItemIconBJNull(udg_DMB , 0 , ( GetConvertedPlayerId(GetOwningPlayer(udg_A_common_HERO)) + 1 ) , udg_S12_Picture[GetUnitPointValue(udg_A_common_HERO)])
    // h除`魂
    set udg_Delete_time2=( udg_Delete_time2 + 1 )
    call ConditionalTriggerExecute(gg_trg_Delete_Delete_time1_time2)
    // R守事件登
    call TriggerRegisterUnitEvent(gg_trg_omamori_Check, GetSoldUnit(), EVENT_UNIT_SPELL_EFFECT)
    // 英雄相P事件登
    call TriggerRegisterUnitEvent(gg_trg_Hero_Die_and_Revive, GetSoldUnit(), EVENT_UNIT_DEATH)
    call TriggerRegisterUnitEvent(gg_trg_Hero_Assist_kill, GetSoldUnit(), EVENT_UNIT_DEATH)
    call TriggerRegisterUnitEvent(gg_trg_Hero_Assist_start, GetSoldUnit(), EVENT_UNIT_DAMAGED)
    call TriggerRegisterUnitEvent(gg_trg_Hero_Assist_play, GetSoldUnit(), EVENT_UNIT_DEATH)
    call TriggerRegisterUnitEvent(gg_trg_Kill_First_Blood, GetSoldUnit(), EVENT_UNIT_DEATH)
    call TriggerRegisterUnitEvent(gg_trg_Kill_Combo, GetSoldUnit(), EVENT_UNIT_DEATH)
    // 明智秀??
    call TriggerRegisterUnitEvent(gg_trg_C10W_PLAY, GetSoldUnit(), EVENT_UNIT_DAMAGED)
    // 休眠d醒系y
    call ConditionalTriggerExecute(udg_Hero_Trigger[GetUnitPointValue(GetSoldUnit())])
    call FlushChildHashtable(YDHT, GetHandleId(GetTriggeringTrigger()) * ydl_localvar_step)
endfunction
