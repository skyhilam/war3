function Trig_tactic_MP_increaseActions takes nothing returns nothing
    // 使用犒p鸱??判啾力是否大於30
    if ( ( GetUnitStateSwap(UNIT_STATE_MAX_MANA, GetTriggerUnit()) >= 150.00 ) ) then
        set udg_tmpPF=YDWEGetForceOfPlayerNull(GetOwningPlayer(GetTriggerUnit()))
        call QuestMessageBJ(udg_tmpPF, bj_QUESTMESSAGE_WARNING, "TRIGSTR_2785")
        call DestroyForce(udg_tmpPF)
    else
        if ( ( GetPlayerState(GetOwningPlayer(GetTriggerUnit()), PLAYER_STATE_RESOURCE_LUMBER) >= IAbsBJ(udg_HIGH) ) ) then
            // 兵力足 t以N的方式增加友鸱法力
            call AdjustPlayerStateBJ(udg_HIGH, GetOwningPlayer(GetTriggerUnit()), PLAYER_STATE_RESOURCE_LUMBER)
            if ( ( IsPlayerAlly(GetOwningPlayer(GetTriggerUnit()), Player(0)) == true ) ) then
                set bj_forLoopAIndex=0
                set bj_forLoopAIndexEnd=4
                loop
                    exitwhen bj_forLoopAIndex > bj_forLoopAIndexEnd
                    call SetPlayerTechResearchedSwap('Rews', ( GetPlayerTechCountSimple('Rews', ConvertedPlayer(( GetForLoopIndexA() + 1 ))) + 1 ), ConvertedPlayer(( GetForLoopIndexA() + 1 )))
                    set bj_forLoopAIndex=bj_forLoopAIndex + 1
                endloop
            else
                set bj_forLoopAIndex=0
                set bj_forLoopAIndexEnd=4
                loop
                    exitwhen bj_forLoopAIndex > bj_forLoopAIndexEnd
                    call SetPlayerTechResearchedSwap('Rews', ( GetPlayerTechCountSimple('Rews', ConvertedPlayer(( GetForLoopIndexA() + 6 ))) + 1 ), ConvertedPlayer(( GetForLoopIndexA() + 6 )))
                    set bj_forLoopAIndex=bj_forLoopAIndex + 1
                endloop
            endif
        else
            // 否t @示兵力不足
            set udg_tmpPF=YDWEGetForceOfPlayerNull(GetOwningPlayer(GetTriggerUnit()))
            call QuestMessageBJ(udg_tmpPF, bj_QUESTMESSAGE_WARNING, "TRIGSTR_11547")
            call DestroyForce(udg_tmpPF)
        endif
    endif
    // 接著 所有玩者鸱ㄟM入CD??即他都施展此技??先P]|l以免重覆|??
    call DisableTrigger(GetTriggeringTrigger())
    if ( ( IsPlayerAlly(GetOwningPlayer(GetTriggerUnit()), Player(0)) == true ) ) then
        set bj_forLoopAIndex=0
        set bj_forLoopAIndexEnd=4
        loop
            exitwhen bj_forLoopAIndex > bj_forLoopAIndexEnd
            call IssueImmediateOrder(udg_tactic_unit1[GetForLoopIndexA()], "channel")
            set bj_forLoopAIndex=bj_forLoopAIndex + 1
        endloop
    else
        set bj_forLoopAIndex=0
        set bj_forLoopAIndexEnd=4
        loop
            exitwhen bj_forLoopAIndex > bj_forLoopAIndexEnd
            call IssueImmediateOrder(udg_tactic_unit2[GetForLoopIndexA()], "channel")
            set bj_forLoopAIndex=bj_forLoopAIndex + 1
        endloop
    endif
    // 1秒後 重新_⒂|l
    call EnableTrigger(GetTriggeringTrigger())
endfunction
