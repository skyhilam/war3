//===========================================================================
function Trig_C05W_PLAYActions takes nothing returns nothing
    local real r1
    local real r2
    local unit u2= udg_Lighting_u2
    local unit u3
    local real x1= GetUnitX(udg_Lighting_u1)
    local real y1= GetUnitY(udg_Lighting_u1)
    local real x2= GetUnitX(udg_Lighting_u2)
    local real y2= GetUnitY(udg_Lighting_u2)
    local real dx= ( x2 - x1 )
    local real dy= ( y2 - y1 )
    if ( ( SquareRoot(( ( dx * dx ) + ( dy * dy ) )) > 1000.00 ) ) then
        set r1=1000.00
    else
        set r1=( SquareRoot(( ( dx * dx ) + ( dy * dy ) )) )
    endif
    set r2=( 1000.00 - r1 )
    set r1=( r1 * ( 0.18 + ( 0.17 * I2R(GetUnitAbilityLevel(udg_Lighting_u1, 'A0HG')) ) ) )
    // ----------------------------------------
    set u3=CreateUnit(Player(PLAYER_NEUTRAL_PASSIVE), 'h01E', GetUnitX(udg_Lighting_u1), GetUnitY(udg_Lighting_u1), 0)
    call ShowUnit(u3, false)
    call UnitApplyTimedLife(u3, 'BHwe', 3.00)
    call UnitAddAbility(u3, 'A0HN')
    call IssueTargetOrder(u3, "ensnare", udg_Lighting_u2)
    // ----------------------------------------
    set r2=( ( r2 / 200.00 ) + 1.00 )
    call YDWEPolledWaitNull(( I2R(R2I(r2)) * udg_C05W_stun[GetUnitAbilityLevel(udg_Lighting_u1, 'A0HG')] ))
    // ----------------------------------------
    call UnitRemoveAbility(u2, 'B086')
    // ----------------------------------------
    set u3=null
    set u2=null
endfunction
