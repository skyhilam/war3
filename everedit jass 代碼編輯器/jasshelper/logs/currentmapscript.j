        call PauseTimer(LoadTimerHandle(YDHT, GetHandleId(GetTriggeringTrigger()) * ydl_localvar_step, 0x312C4181))
        call FlushChildHashtable(YDHT, ((GetHandleId((LoadTimerHandle(YDHT, GetHandleId(GetTriggeringTrigger()) * ydl_localvar_step, 0x312C4181)))))) // INLINED!!
        call DestroyTimer(LoadTimerHandle(YDHT, GetHandleId(GetTriggeringTrigger()) * ydl_localvar_step, 0x312C4181))
    else
