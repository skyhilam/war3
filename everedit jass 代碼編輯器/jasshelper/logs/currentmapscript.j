function Trig_ice_shieldConditions takes nothing returns boolean
    return ( ( GetSpellAbilityId() == 'A0R1' ) and ( GetUnitTypeId(GetTriggerUnit()) != 'nhym' ) )
endfunction
function Trig_ice_shieldActions takes nothing returns nothing
    local unit U= GetTriggerUnit()
    call YDWEPolledWaitNull(8.00)
    if ( ( IsUnitPausedBJ(U) == false ) ) then
        if ( ( IsUnitAliveBJ(U) == true ) ) then
            set bj_forLoopAIndex=1
            set bj_forLoopAIndexEnd=6
            loop
                exitwhen bj_forLoopAIndex > bj_forLoopAIndexEnd
                set udg_tmpItem=UnitItemInSlotBJ(( U ), GetForLoopIndexA())
                if ( ( GetItemTypeId(udg_tmpItem) == 'I02G' ) ) then
                    call RemoveItem(udg_tmpItem)
                    call UnitAddItemToSlotById(U, 'I02G', ( GetForLoopIndexA() - 1 ))
                else
                endif
                set bj_forLoopAIndex=bj_forLoopAIndex + 1
            endloop
        else
        endif
    else
    endif
    set U=null
endfunction
