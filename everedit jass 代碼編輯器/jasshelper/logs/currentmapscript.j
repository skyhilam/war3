function Trig_lightningFunc001T takes nothing returns nothing
    local timer t= GetExpiredTimer()
    local unit u1= LoadUnitHandle(YDHT, GetHandleId(t), 0x1BF14788)
    local unit u2= LoadUnitHandle(YDHT, GetHandleId(t), 0x2D5DEA42)
    local location p1= LoadLocationHandle(YDHT, GetHandleId(t), 0x50525CFC)
    local location p2= LoadLocationHandle(YDHT, GetHandleId(t), 0xDEFC4000)
    local lightning l= LoadLightningHandle(YDHT, GetHandleId(t), 0x9B1A6867)
    local trigger tr= LoadTriggerHandle(YDHT, GetHandleId(t), 0x66BD1F81)
    local integer i= LoadInteger(YDHT, GetHandleId(t), 0x312C4181)
    local integer i2= LoadInteger(YDHT, GetHandleId(t), 0x214C62CC)
    local real x1= GetUnitX(u1)
    local real x2= GetUnitX(u2)
    local real y1= GetUnitY(u1)
    local real y2= GetUnitY(u2)
    local real z1
    local real z2
    if ( ( ( IsUnitType(u1, UNIT_TYPE_DEAD) == true ) or ( IsUnitType(u2, UNIT_TYPE_DEAD) == true ) or ( i == 0 ) ) ) then
        call MoveLocation(p1, GetUnitX(u1), GetUnitY(u1))
        call MoveLocation(p2, GetUnitX(u2), GetUnitY(u2))
        set z1=GetLocationZ(p1) + GetUnitFlyHeight(u1)
        set z2=GetLocationZ(p2) + GetUnitFlyHeight(u2)
        call RemoveLocation(p1)
        call RemoveLocation(p2)
        call DestroyLightning(l)
	call FlushChildHashtable(YDHT, GetHandleId(t))
        call DestroyTimer(t)
    else
        //========================================================
        call SaveInteger(YDHT, GetHandleId(t), 0x312C4181, ( i - 1 ))
        //========================================================
        call MoveLocation(p1, GetUnitX(u1), GetUnitY(u1))
        call MoveLocation(p2, GetUnitX(u2), GetUnitY(u2))
        set z1=GetLocationZ(p1) + GetUnitFlyHeight(u1)
        set z2=GetLocationZ(p2) + GetUnitFlyHeight(u2)
        //========================================================
        call MoveLightningEx(l, false, x1, y1, z1, x2, y2, z2)
        //========================================================
    endif
            if ( ( ModuloInteger(i, i2) == 0 ) ) then
            set udg_Lighting_u1=u1
            set udg_Lighting_u2=u2
            call TriggerExecute(tr)
	    else
	    endif
set tr=null
set u1=null
set u2=null
set t=null
set p1=null
set p2=null
set l=null
endfunction
