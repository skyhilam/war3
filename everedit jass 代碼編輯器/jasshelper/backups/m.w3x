local integer h=i - i / 0x2000 * 0x2000
local integer k=h
loop
exitwhen udg_KnockBack_HASH_Boolean[h]
set h=GetRandomInt(0x0, 0x1FFF)
endloop
if h != k then
loop
if udg_KnockBack_HASH_Handle[k] == i then
return k
endif
set k=udg_KnockBack_HASH_NIndex[k]
exitwhen udg_KnockBack_HASH_NIndex[k] == 0
endloop
set udg_KnockBack_HASH_NIndex[k]=h
set udg_KnockBack_HASH_RIndex[h]=k
endif
set udg_KnockBack_HASH_Handle[h]=i
set udg_KnockBack_HASH_Boolean[h]=false
return h
endfunction
function KnockBack_GetID takes handle v returns integer
local integer i=GetHandleId(v)
local integer h=i - i / 0x2000 * 0x2000
loop
exitwhen udg_KnockBack_HASH_Handle[h] == i
set h=udg_KnockBack_HASH_NIndex[h]
endloop
return h
endfunction
function KnockBack_ClsID takes integer h returns nothing
set udg_KnockBack_HASH_Handle[h]=0
set udg_KnockBack_HASH_Boolean[h]=true
set udg_KnockBack_HASH_NIndex[udg_KnockBack_HASH_RIndex[h]]=udg_KnockBack_HASH_NIndex[h]
set udg_KnockBack_HASH_RIndex[udg_KnockBack_HASH_NIndex[h]]=udg_KnockBack_HASH_RIndex[h]
set udg_KnockBack_HASH_NIndex[h]=0
endfunction
function KnockBack_Timer takes nothing returns nothing
local timer t=GetExpiredTimer()
local integer i=KnockBack_GetID(t)
local unit u=udg_KnockBack_Handle_u[i]
local real d=udg_KnockBack_Handle_d[i]
local real a=udg_KnockBack_Handle_a[i]
local real x=GetUnitX(u) + d * Cos(a * bj_DEGTORAD)
local real y=GetUnitY(u) + d * Sin(a * bj_DEGTORAD)
local real dx
local real dy
local boolean b=false
if udg_KnockBack_Handle_b1[i] then
if not IsTerrainPathable(x, y, PATHING_TYPE_FLYABILITY) then
call SetUnitX(u, x)
call SetUnitY(u, y)
else
set b=true
endif
else
call SetUnitPosition(u, x, y)
set dx=GetUnitX(u) - x
set dy=GetUnitY(u) - y
if SquareRoot(dx * dx + dy * dy) > d then
if udg_KnockBack_Handle_b2[i] then
set udg_KnockBack_Handle_a[i]=bj_RADTODEG * Atan2(GetUnitY(u) - y, GetUnitX(u) - x)
else
set b=true
endif
endif
endif
set udg_KnockBack_Handle_I[i]=udg_KnockBack_Handle_I[i] + udg_KnockBack_Handle_i[i]
if udg_KnockBack_Handle_I[i] >= udg_KnockBack_Handle_t[i] or b then
if udg_KnockBack_Handle_Trigger[i] != null then
set udg_KnockBack_Unit=u
set udg_KnockBack_Time=udg_KnockBack_Handle_I[i]
set udg_KnockBack_Boolean1=b
if TriggerEvaluate(udg_KnockBack_Handle_Trigger[i]) then
call TriggerExecute(udg_KnockBack_Handle_Trigger[i])
endif
endif
call PauseTimer(t)
call DestroyTimer(t)
call KnockBack_ClsID(i)
else
set udg_KnockBack_Handle_d[i]=d * udg_KnockBack_Handle_r[i]
endif
set t=null
set u=null
endfunction
function KnockBack takes nothing returns nothing
local timer t=CreateTimer()
local integer i=KnockBack_SetID(t)
set udg_KnockBack_Handle_I[i]=0
set udg_KnockBack_Handle_u[i]=udg_KnockBack_Unit
set udg_KnockBack_Handle_d[i]=udg_KnockBack_Dist
set udg_KnockBack_Handle_a[i]=udg_KnockBack_Angle
set udg_KnockBack_Handle_t[i]=udg_KnockBack_Time
set udg_KnockBack_Handle_r[i]=udg_KnockBack_Rate
set udg_KnockBack_Handle_i[i]=udg_KnockBack_Interval
set udg_KnockBack_Handle_b1[i]=udg_KnockBack_Boolean1
set udg_KnockBack_Handle_b2[i]=udg_KnockBack_Boolean2
set udg_KnockBack_Handle_Trigger[i]=udg_KnockBack_Trigger
call TimerStart(t, udg_KnockBack_Interval, true, function KnockBack_Timer)
call ExecuteFunc("KnockBack_Cls")
set t=null
endfunction
