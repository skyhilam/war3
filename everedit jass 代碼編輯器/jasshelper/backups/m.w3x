function Trig_MpqResultCheck_Actions takes nothing returns nothing
local boolean b=false
set bj_forLoopAIndex=1
set bj_forLoopAIndexEnd=12
loop
exitwhen bj_forLoopAIndex > bj_forLoopAIndexEnd
if GetPlayerSlotState(ConvertedPlayer(GetForLoopIndexA())) == PLAYER_SLOT_STATE_PLAYING and GetPlayerController(ConvertedPlayer(GetForLoopIndexA())) == MAP_CONTROL_USER and GetPlayerState(ConvertedPlayer(GetForLoopIndexA()), PLAYER_STATE_OBSERVER) == 0 then
set udg_MpqVersion[GetForLoopIndexA()]=GetStoredInteger(udg_GC, "Xmpq", I2S(GetForLoopIndexA()))
if GetStoredInteger(udg_GC, "Xmpq", I2S(GetForLoopIndexA())) == 9999 then
set b=true
endif
endif
set bj_forLoopAIndex=bj_forLoopAIndex + 1
endloop
if not b then
call DisableTrigger(GetTriggeringTrigger())
call DestroyTrigger(GetTriggeringTrigger())
set bj_forLoopAIndex=1
set bj_forLoopAIndexEnd=12
loop
exitwhen bj_forLoopAIndex > bj_forLoopAIndexEnd
if GetPlayerSlotState(ConvertedPlayer(GetForLoopIndexA())) == PLAYER_SLOT_STATE_PLAYING and GetPlayerController(ConvertedPlayer(GetForLoopIndexA())) == MAP_CONTROL_USER and GetPlayerState(ConvertedPlayer(GetForLoopIndexA()), PLAYER_STATE_OBSERVER) == 0 then
if ( Trig_MpqResultCheck_Func007Func002C() ) then
set udg_MinMpqVersion=udg_MpqVersion[GetForLoopIndexA()]
else
endif
if ( Trig_MpqResultCheck_Func007Func003C() ) then
call DisplayTextToForce(GetPlayersAll(), ( ( GetPlayerName(ConvertedPlayer(GetForLoopIndexA())) + "未安裝極限資料片" ) ))
else
if ( Trig_MpqResultCheck_Func007Func003Func001C() ) then
call DisplayTextToForce(GetPlayersAll(), ( ( GetPlayerName(ConvertedPlayer(GetForLoopIndexA())) + "的極限資料片版本是：" ) + " 2.0 或 2.1" ))
else
call DisplayTextToForce(GetPlayersAll(), ( ( GetPlayerName(ConvertedPlayer(GetForLoopIndexA())) + "的極限資料片版本是：" ) + R2SW(( I2R(udg_MpqVersion[GetForLoopIndexA()]) / 10.00 ), 4, 1) ))
endif
endif
endif
set bj_forLoopAIndex=bj_forLoopAIndex + 1
endloop
if ( Trig_MpqResultCheck_Func012C() ) then
call TriggerExecute(gg_trg_Mpq_Action_WithoutMpq)
else
endif
if ( Trig_MpqResultCheck_Func014C() ) then
call TriggerExecute(gg_trg_Mpq_Action_Mpq10)
else
endif
if ( Trig_MpqResultCheck_Func016C() ) then
call TriggerExecute(gg_trg_Mpq_Action_Mpq20)
else
endif
if ( Trig_MpqResultCheck_Func018C() ) then
call TriggerExecute(gg_trg_Mpq_Action_Mpq22)
else
endif
if ( Trig_MpqResultCheck_Func019C() ) then
call TriggerExecute(gg_trg_Mpq_Action_Mpq01)
else
endif
else
call PolledWait(1.00)
call ConditionalTriggerExecute(GetTriggeringTrigger())
endif
endfunction
