function ATK takes nothing returns boolean
local unit u=GetOrderedUnit()
local unit u2=GetOrderTargetUnit()
if GetIssuedOrderId() == 851983 and IsUnitAlly(u2, GetOwningPlayer(u)) and GetOwningPlayer(u2) != Player(15) then
call PauseUnit(u, true)
call PauseUnit(u, false)
call IssueImmediateOrder(u, "stop")
endif
set u=null
set u2=null
return false
endfunction
function ATK_Init takes nothing returns nothing
local integer i=0
local trigger t=CreateTrigger()
loop
exitwhen i > 15
call TriggerRegisterPlayerUnitEvent(t, Player(i), EVENT_PLAYER_UNIT_ISSUED_TARGET_ORDER, null)
set i=i + 1
endloop
call TriggerAddCondition(t, Condition(function ATK))
set t=null
endfunction
