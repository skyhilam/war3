	    if IsPlayerEnemy(GetLocalPlayer(), p) then
	        // Use only local code (no net traffic) within this block to avoid desyncs.
	        call SetTextTagVisibility(tt, false)
	    endif
	set tt=null
	set s=null
	set p=null
    set u1=null
    set u2=null
endfunction
