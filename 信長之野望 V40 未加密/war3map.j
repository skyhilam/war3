    	call SaveBoolean(YDHT, h2i, 0xA3098AE2, b)
else
	if ( u2 != null ) then
    	set reach=( LoadInteger(YDHT, h2i2, 0x9C0F555E) - i )
	
    	set u3=LoadUnitHandle(YDHT, h2i2, reach)
