		set B02R_U[i]=GetSpellTargetUnit()
		
		call SaveUnitHandle(YDHT, h2i, 0x9C0F555E, GetTriggerUnit())
		call TimerStart(t, 0.10, true, function Trig_B02RFunc001T)
		    
	set t=null
	set spell_caster=null
endfunction
