    call SaveTriggerHandle(YDHT, h2i, 0x66BD1F81, udg_KnockBack_Trigger)
    call SaveInteger(YDHT, h2i, 555, udg_KnockBack_I2)
    
    call SaveReal(YDHT, h2i, 0xDD66C616, 0.00)
    
