				    	set udg_A05R_SE[i]=AddSpecialEffectTarget("Abilities\\Spells\\Orc\\SpiritLink\\SpiritLinkTarget.mdl", u1, "weapon")
				    	set udg_A05R_B[i]=false
					endif
			endif
	endif
set u1=null
endfunction
