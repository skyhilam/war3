function YDWEItemAbilitySystemByAbility_CON takes nothing returns boolean
    local unit l__hero= GetTriggerUnit()
    local item it=GetManipulatedItem()
    local trigger trg=(LoadTriggerHandle(YDHT, StringHash((I2S((GetHandleId((l__hero)))) )), StringHash(( "MulItemSlotByAbility")))) // INLINED!!
    local integer itid=GetItemTypeId(it)
    local integer index= (LoadInteger(YDHT, StringHash((I2S(itid) )), StringHash(( "AbilityStorageIndex")))) // INLINED!!
    local integer array skills
    local integer i=0
    local integer N=(LoadInteger(YDHT, StringHash((I2S((GetHandleId((trg)))) )), StringHash(( "ItemSlotNum")))) // INLINED!!
    local boolean OnOff=(LoadBoolean(YDHT, StringHash((I2S((GetHandleId((l__hero)))) )), StringHash(( "YDWEItemAbilitySystemTriggerClose")))) // INLINED!!
    if index == 0 or OnOff == true then
        set it=null
        set l__hero=null
        set trg=null
        return false
    endif
    loop
        set i=i + 1
        exitwhen i > index
        set skills[i]=(LoadInteger(YDHT, StringHash((I2S(itid) )), StringHash(( "AbilityStorage" + I2S(i))))) // INLINED!!
        if GetTriggerEventId() == EVENT_PLAYER_UNIT_DROP_ITEM then
            call UnitRemoveAbility(l__hero, skills[i])
          elseif GetTriggerEventId() == EVENT_PLAYER_UNIT_PICKUP_ITEM then
            call UnitAddAbility(l__hero, skills[i])
            call UnitMakeAbilityPermanent(l__hero, true, skills[i])
        endif
    endloop
    set it=null
    set l__hero=null
    set trg=null
    return false
endfunction
 function YDWEItemAbilitySystemInit takes nothing returns nothing
