    	call SaveInteger(YDHT, h2i, 0x9C0F555E, i)
    
    	set x=GetUnitX(u1)
		set y=GetUnitY(u1)
		set spellT=0
		set a=GetUnitFacing(u1) * bj_DEGTORAD
    	set x1=x + 40.00 * I2R(i) * Cos(a)
    	set y1=y + 40.00 * I2R(i) * Sin(a)
    	call GroupEnumUnitsInRange(udg_Group[3], x1, y1, 122.00, null)
    		loop
